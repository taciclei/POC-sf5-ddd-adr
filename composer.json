{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "filp/whoops": "^2.7",
        "friendsofsymfony/rest-bundle": "^2.7",
        "jms/serializer-bundle": "^3.7",
        "laminas/laminas-hydrator": "^4.0",
        "nelmio/api-doc-bundle": "^4.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "4.0.*",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.4",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "4.4",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4",
        "symfony/framework-bundle": "4.4",
        "symfony/messenger": "4.4",
        "symfony/property-access": "4.3.*",
        "symfony/property-info": "4.3.*",
        "symfony/serializer": "4.4",
        "symfony/twig-bundle": "4.4",
        "symfony/validator": "4.4",
        "symfony/web-profiler-bundle": "4.4",
        "symfony/yaml": "4.4",
        "twig/extra-bundle": "^1.0||^2.0",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.7",
        "behat/mink": "^1.8",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.3",
        "doctrine/data-fixtures": "^1.4",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3.1",
        "friends-of-behat/symfony-extension": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^0.12.5",
        "phpstan/phpstan-symfony": "^0.12.10",
        "symfony/debug-bundle": "^4.3",
        "symfony/maker-bundle": "^1.21",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "^4.3",
        "symfony/var-dumper": "^4.3",
        "webmozart/assert": "^1.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ecommerce\\": "src/Ecommerce/",
            "Infrastructure\\": "src/Infrastructure/",
            "Application\\": "src/Application/",
            "App\\": "src/"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
